{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "Chart.js@3.6.1",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "google-palette@1.1.0",
      "destination": "wwwroot/lib/google-palette/"
    },
    {
      "provider": "unpkg",
      "library": "sortablejs@1.14.0",
      "destination": "wwwroot/lib/sortablejs/",
      "files": [
        "LICENSE",
        "package.json",
        "README.md",
        "Sortable.js",
        "Sortable.min.js"
      ]
    }
  ,
{
  "provider": "unpkg",
  "library": "jquery-sortablejs@1.0.1",
  "destination": "wwwroot/lib/jquery-sortablejs/"
}
]
}